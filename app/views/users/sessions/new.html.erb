<div class="container">
<h2 class="thin teal-text center-aligned" >Ingresar</h2>
<br>
<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="input-field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, required: true %>
  </div>

  <div class="input-field">
    <%= f.label :password, "Contraseña" %><br />
    <%= f.password_field :password, autocomplete: "off", required: true %>
  </div>

  <% if devise_mapping.rememberable? -%>
    <div class="input-field">
      <%= f.check_box :remember_me %>
      <%= f.label :remember_me, "Recordarme" %>
    </div>
  <% end %>
<br>
  <div class="actions">
  <button type="submit" class="btn waves">Iniciar Sesión</button>
  </div>
<% end %>

<%= render "users/shared/links" %>
</div>