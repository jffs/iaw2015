<html>
<body>
<div class="container">
  <br><h2 class="teal-text thin">Registro</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

     <div class="row">
        <div class="input-field col s6">
          <%= f.label :nombre %>
          <%= f.text_field :nombre, class: 'validate',required: true%>
        </div>
        <div class="input-field col s6">
          <%= f.label :apellido %>
          <%= f.text_field :apellido, class: 'form-control',required: true %>
        </div>
      </div>
   <div class="row">
        <div class="input-field col s6">
        <%= f.label "Nombre de Usuario*" %>
        <%= f.text_field :username,required: true%>
        </div>
        <div class="input-field col s6">
          <%= f.label :password %>
          <%= f.password_field  :password, autocomplete: "off",required: true %>
        </div>
  </div>
  <div class="row">
    <div class="input-field col s6">
      <%= f.label :email%>
      <%= f.email_field :email, autofocus: true, required: true %>
  </div>
  </div>

  <div class="actions">       
  <button type="submit" class="btn waves">Enviar</button>
  </div>
  

  
<% end %>
</div>
</body>
</html>
   
